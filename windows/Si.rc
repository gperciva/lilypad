  MENUITEM "&Kopiraj\tCtrl+C",     CMD_COPY
  MENUITEM "&Prilepi\tCtrl+V",    CMD_PASTE
  MENUITEM "Izbri&ši\tDel",      CMD_DELETE
  MENUITEM SEPARATOR
  MENUITEM "Izberi &vse",       CMD_SELECT_ALL
  MENUITEM "Èas/&Datum\tF5",    CMD_TIME_DATE
  MENUITEM SEPARATOR
