LANGUAGE LANG_SLOVENIAN, SUBLANG_DEFAULT

MAIN_MENU MENU
{
 POPUP "&Datoteka" {
  MENUITEM "&Nova",           CMD_NEW
  MENUITEM "&Odpri ...",             CMD_OPEN
  MENUITEM "&Shrani",             CMD_SAVE
