{
 POPUP "&Fájl" {
  MENUITEM "Ú&j",           CMD_NEW
  MENUITEM "Meg&nyitás...",             CMD_OPEN
  MENUITEM "&Mentés",             CMD_SAVE
  MENUITEM "Mentés má&sként...",       CMD_SAVE_AS
  MENUITEM SEPARATOR
