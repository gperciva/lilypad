MAIN_MENU MENU
{
 POPUP "&Tiedosto" {
  MENUITEM "&Uusi...",		CMD_NEW
  MENUITEM "&Avaa",		CMD_OPEN
  MENUITEM "Ta&lleta",		CMD_SAVE
