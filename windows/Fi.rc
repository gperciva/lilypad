  MENUITEM "&Font...",          CMD_FONT
 }
POPUP "&Etsi" {
  MENUITEM "Etsi...",		CMD_SEARCH
  MENUITEM "Etsi &seuraava\tF3",	CMD_SEARCH_NEXT
 }
POPUP "&Apua" {
  MENUITEM "&About LilyPad...", CMD_HELP_ABOUT_LILYPAD
 }
}

